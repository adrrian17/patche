{
  "name": "patche",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once && concurrently -r npm:dev:web npm:dev:convex",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "npx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "lint": "ultracite check",
    "format": "ultracite fix",
    "lint:fix": "ultracite check && ultracite fix",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@convex-dev/react-query": "^0.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.154.14",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.154.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.6",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@convex-dev/eslint-plugin": "^1.1.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/eslint-config": "^0.3.4",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "~9.2.1",
    "lefthook": "^2.0.16",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "ultracite": "7.0.12",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "ultracite fix"
    ]
  }
}
